{
    "name": "becklyn/ddd-generator-bundle",
    "type": "symfony-bundle",
    "description": "Code generator for common Domain Driven Design patterns in the Becklyn DDD framework.",
    "keywords": [
        "ddd",
        "domain driven design",
        "code generation"
    ],
    "license": "proprietary",
    "authors": [
        {
            "name": "Pascal Gläßer",
            "email": "pascal.glaesser1997@gmail.com",
            "homepage": "https://glassp.dev"
        }
    ],
    "require": {
        "ext-json": "*",
        "symfony/maker-bundle": "^1.21"
    },
    "require-dev": {
        "php": ">=8.0",
        "becklyn/ddd-core": "^2.0",
        "doctrine/orm": "^2.8",
        "phpspec/prophecy": "^1.12",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/config": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\DddGeneratorBundle\\": "src/"
        }
    },
    "scripts": {
        "auto-scripts": {}
    }
}
