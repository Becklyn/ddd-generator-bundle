{
    "name": "201created/ddd-generator-bundle",
    "type": "symfony-bundle",
    "description": "Code generator for common Domain Driven Design patterns in the 201Created DDD framework.",
    "keywords": [
        "ddd",
        "domain driven design",
        "code generation"
    ],
    "version": "1.0",
    "license": "proprietary",
    "authors": [
        {
            "name": "Pascal Gläßer",
            "email": "pascal.glaesser1997@gmail.com",
            "homepage": "https://glassp.dev"
        }
    ],
    "require": {
        "ext-json": "*",
        "symfony/maker-bundle": "^1.21"
    },
    "require-dev": {
        "php": ">=7.4",
        "201created/ddd-core": "1.0.0",
        "doctrine/orm": "^2.8",
        "gedmo/doctrine-extensions": "^3.0",
        "phpspec/prophecy": "^1.12",
        "roave/security-advisories": "dev-master",
        "symfony/config": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/finder": "^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/yaml": "^5.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "C201\\DddGeneratorBundle\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/201created-gmbh/ddd-core"
        },
        {
            "type": "github",
            "url": "https://github.com/201CREATED-GmbH/support"
        }
    ],
    "scripts": {
        "auto-scripts": {}
    }
}
